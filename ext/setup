wget http://archive.apache.org/dist/tomcat/tomcat-7/v7.0.50/bin/apache-tomcat-7.0.50.tar.gz
#wget http://mirror.sdunix.com/apache/tomcat/tomcat-7/v7.0.50/bin/apache-tomcat-7.0.50.tar.gz
ln -fs apache-tomcat-7.*.tar.gz apache-tomcat.tar.gz
tar -xzf apache-tomcat-7.*.tar.gz
rm apache-tomcat
ln -fs apache-tomcat-7.*/ apache-tomcat

